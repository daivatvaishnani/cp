    "from functools import reduce\n",
    "t = int(input())\n",
    "for i in range(0,t):\n",
    "    n = int(input())\n",
    "    nums = [int(num) for num in input().split()]\n",
    "    ans = 0\n",
    "    for i in range(0,n-1):\n",
    "        diff = abs(nums[i]-nums[i+1]) - 1\n",
    "        ans += diff\n",
    "    print(ans)"
